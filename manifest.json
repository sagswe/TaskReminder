{
    "manifest_version": 3,
    "name": "Dev chronology",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
      "action": {
        "default_popup": "html/popup.html",
        "default_icon": "images/pencil.png"
      },
       
      "background": {
        "service_worker": "background.js"
      },
    "permissions": [
      "storage",
      "alarms",
      "notifications"
    ] 
}  